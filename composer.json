{
    "name": "survos-sites/meili",
    "description": "A Symfony application that demonstrates meili-bundle",
    "keywords": ["symfony","meilisearch"],
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "RC",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-pdo_sqlite": "*",
        "ext-sodium": "*",
        "ext-sqlite3": "*",
        "ext-zip": "*",
        "api-platform/core": "^4.2",
        "doctrine/doctrine-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^4.0",
        "doctrine/orm": "^3.4.2",
        "easycorp/easyadmin-bundle": "^4.24",
        "friendsofsymfony/jsrouting-bundle": "dev-master",
        "halaxa/json-machine": "^1.2",
        "inspector-apm/inspector-symfony": "^1.8",
        "league/csv": "^9.24",
        "league/flysystem-aws-s3-v3": "^3.22",
        "league/flysystem-bundle": "^3.3",
        "liip/imagine-bundle": "^2.12",
        "liquid/liquid": "^1.4",
        "meilisearch/meilisearch-php": "dev-main",
        "nelmio/cors-bundle": "^2.3.1",
        "openai-php/client": "^0.18.0",
        "phpdocumentor/reflection-docblock": "^6.0",
        "phpmetrics/phpmetrics": "^3.0",
        "sentry/sentry-symfony": "^4.12|^5.0",
        "sunaoka/ndjson": "^1.1",
        "survos/auth-bundle": "^2.0",
        "survos/bootstrap-bundle": "^2.0",
        "survos/command-bundle": "^2.0",
        "survos/core-bundle": "^2.0",
        "survos/crawler-bundle": "^2.0",
        "survos/ez-bundle": "^2.0",
        "survos/html-prettify-bundle": "^2.0",
        "survos/import-bundle": "^2.0",
        "survos/jsonl-bundle": "^2.0",
        "survos/meili-bundle": "^2.0",
        "survos/scraper-bundle": "^2.0",
        "survos/simple-datatables-bundle": "^2.0",
        "survos/state-bundle": "^2.0",
        "survos/wiki-bundle": "^2.0",
        "symfony/asset": "^7.4||^8.0",
        "symfony/asset-mapper": "^7.4||^8.0",
        "symfony/console": "^8.0",
        "symfony/doctrine-messenger": "^7.4||^8.0",
        "symfony/dotenv": "^7.4||^8.0",
        "symfony/expression-language": "^7.4||^8.0",
        "symfony/flex": "^2.4.1",
        "symfony/form": "^7.4||^8.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/http-client": "^8.0",
        "symfony/intl": "^7.4||^8.0",
        "symfony/mailer": "^7.4||^8.0",
        "symfony/mime": "^7.4||^8.0",
        "symfony/monolog-bundle": "^4.0",
        "symfony/notifier": "^7.4||^8.0",
        "symfony/object-mapper": "^8.0",
        "symfony/process": "^7.4||^8.0",
        "symfony/property-access": "^7.4||^8.0",
        "symfony/property-info": "^7.4||^8.0",
        "symfony/redis-messenger": "^7.4||^8.0",
        "symfony/runtime": "^7.4||^8.0",
        "symfony/security-bundle": "^7.4||^8.0",
        "symfony/serializer": "^7.4||^8.0",
        "symfony/stimulus-bundle": "^2.30",
        "symfony/string": "^7.4||^8.0",
        "symfony/translation": "^7.4||^8.0",
        "symfony/twig-bundle": "^7.4||^8.0",
        "symfony/uid": "^7.4||^8.0",
        "symfony/ux-chartjs": "^2.31",
        "symfony/ux-icons": "^2.21",
        "symfony/ux-twig-component": "^2.13",
        "symfony/validator": "^7.4||^8.0",
        "symfony/web-link": "^7.4||^8.0",
        "symfony/yaml": "^7.4||^8.0",
        "twig/extra-bundle": "^3.7.1",
        "twig/intl-extra": "^3.8",
        "twig/markdown-extra": "^3.8",
        "twig/twig": "^3.13",
        "zenstruck/messenger-monitor-bundle": "^0.6.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "endroid/installer": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "survos/installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "fos:js-routing:dump --format=js --target=public/js/fos_js_routes.js --callback=\"export default  \"": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": "false"
        },
        "paas": {
            "php-config": [
                "memory_limit=1024M",
                "max_execution_time=60",
                "post_max_size=128M",
                "upload_max_filesize=128M"
            ]
        }
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.4",
        "doctrine/doctrine-fixtures-bundle": "^3.4.4||^4.0",
        "friendsofphp/php-cs-fixer": "^3.70",
        "jawira/doctrine-diagram-bundle": "^1.0",
        "nette/php-generator": "^4.1",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^12.1",
        "rector/rector": "^2.0",
        "roave/security-advisories": "dev-latest",
        "spaze/phpstan-disallowed-calls": "^4.0",
        "survos/code-bundle": "dev-main",
        "survos/deployment-bundle": "^2.0",
        "symfony/browser-kit": "^7.4||^8.0",
        "symfony/css-selector": "^7.4||^8.0",
        "symfony/debug-bundle": "^7.4||^8.0",
        "symfony/maker-bundle": "^1.51.1",
        "symfony/panther": "dev-main",
        "symfony/phpunit-bridge": "^7.4||^8.0",
        "symfony/stopwatch": "^7.4||^8.0",
        "symfony/web-profiler-bundle": "^7.4||^8.0",
        "symplify/easy-coding-standard": "^13.0",
        "vincentlanglet/twig-cs-fixer": "^3.0",
        "yethee/tiktoken": "^1.0"
    },
    "repositories": {

    }
}
